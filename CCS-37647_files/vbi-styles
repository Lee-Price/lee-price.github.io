      -ms-transform: skew(-13deg, 0deg) translateX(100px);
      -webkit-transform: skew(-13deg, 0deg) translateX(100px);
      transform: skew(-13deg, 0deg) translateX(100px);
    }
  }
  @keyframes bounce-slick-next-out {
    from {
      -ms-transform: skew(-13deg, 0deg) translateX(100px);
      -webkit-transform: skew(-13deg, 0deg) translateX(100px);
      transform: skew(-13deg, 0deg) translateX(100px);
    }
    to {
      -ms-transform: skew(-13deg, 0deg) translateX(95px);
      -webkit-transform: skew(-13deg, 0deg) translateX(95px);
      transform: skew(-13deg, 0deg) translateX(95px);
    }
  }
  .tab-pane .vbi-carousel-inner .carousel-item-listing-container .slick-arrow, .tab-pane .vbi-carousel-inner .frigidaire-interactive-banner .slick-arrow {
    display: block;
    position: absolute;
    content: '';
    z-index: 2;
    background: #58C8E7;
    padding: 32px 18px;
    top: 36%;
    transition: 0.5s ease;
    opacity: 1;
  }
  .tab-pane .vbi-carousel-inner .carousel-item-listing-container .slick-arrow:hover, .tab-pane .vbi-carousel-inner .frigidaire-interactive-banner .slick-arrow:hover {
    opacity: .8;
  }
  .tab-pane .vbi-carousel-inner .carousel-item-listing-container .slick-prev, .tab-pane .vbi-carousel-inner .frigidaire-interactive-banner .slick-prev {
    left: 84px;
    -ms-transform: skew(-13deg, 0deg) translateX(-100px);
    -webkit-transform: skew(-13deg, 0deg) translateX(-100px);
