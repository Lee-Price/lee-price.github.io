  /* FF3.6-15 */
  background: -webkit-radial-gradient(center, ellipse cover, #fcfcfc 0%, #e0e0e0 100%);
  /* Chrome10-25,Safari5.1-6 */
  background: radial-gradient(ellipse at center, #fcfcfc 0%, #e0e0e0 100%);
  /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fcfcfc', endColorstr='#e0e0e0',GradientType=1 );
  /*background: -moz-radial-gradient(center, ellipse cover, rgba(0,0,0,0) 0%, rgba(82,82,82,0) 48%, rgba(170,170,170,0.65) 100%);*/
  /* FF3.6-15 */
  /*background: -webkit-radial-gradient(center, ellipse cover, rgba(0,0,0,0) 0%,rgba(82,82,82,0) 48%,rgba(170,170,170,0.65) 100%);*/
  /* Chrome10-25,Safari5.1-6 */
  /*background: radial-gradient(ellipse at center, rgba(0,0,0,0) 0%,rgba(82,82,82,0) 48%,rgba(170,170,170,0.65) 100%);*/
  /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
  /*filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00000000', endColorstr='#a6aaaaaa',GradientType=1 );*/
  /* IE6-9 fallback on horizontal gradient */
}

